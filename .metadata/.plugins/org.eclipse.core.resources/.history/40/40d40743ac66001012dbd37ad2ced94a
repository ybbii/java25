
public class ForTest2 {
	
	public static void main(String[] args) {
	
		// 가로버전
		for (int i=2; i<10; i++) {
			System.out.printf("%d단 :\t", i);
			for (int j=1; j<10; j++) {
				System.out.printf("%d X %d = %d\t", i, j, i*j);
			}
			
			System.out.println();
		}
		
		
		
		// 세로버전
		for (int j=1; j<10; j++) {
			for (int i=2; i<10; i++) {
				System.out.printf("%d X %d = %d\t", i, j, i*j);
			}
			
			System.out.println();
		}
	}
}
